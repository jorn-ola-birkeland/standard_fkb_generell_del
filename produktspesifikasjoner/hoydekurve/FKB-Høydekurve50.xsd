        <sequence>
          <element name="senterlinje" type="gml:CurvePropertyType">
            <annotation>
              <documentation>centerline: forløp som følger objektets sentrale del</documentation>
              <appinfo>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">Kurve</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="høyde" type="string">
            <annotation>
              <documentation>height: angir forsenkningskurvens  høyde over høydereferansen i meter- og oppgis som et desimalt tall hvis nødvendig</documentation>
              <appinfo>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">H</taggedValue>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">HØYDE</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="opphav" type="fkbhk:OpphavPropertyType">
            <annotation>
              <documentation>beskriver objektets opphav og forteller om objektet sikkert gjengir terrenget</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
