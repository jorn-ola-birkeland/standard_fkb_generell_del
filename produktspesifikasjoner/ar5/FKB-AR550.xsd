  <element name="ArealressursFlate" substitutionGroup="app:KvalitetPåkrevd" type="app:ArealressursFlateType">
    <annotation>
      <documentation>AreaResourceSurface: et sammenhengende areal som er tilordnet de samme egenskapsverdiene i henhold til et Arealressursklassifikasjonssystem</documentation>
    </annotation>
  </element>
  <complexType name="ArealressursFlateType">
    <complexContent>
      <extension base="app:KvalitetPåkrevdType">
        <sequence>
          <element name="område" type="gml:SurfacePropertyType">
            <annotation>
              <documentation>extent: objektets utstrekning</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="posisjon" type="gml:PointPropertyType">
            <annotation>
              <documentation>sted som objektet eksisterer på</documentation>
            </annotation>
          </element>
          <element name="arealtype" type="gml:CodeType">
            <annotation>
              <documentation>areaType: hovedinndeling etter kriterier for vegetasjon, naturlig drenering og kulturpåvirkning</documentation>
              <appinfo>
                <defaultCodeSpace xmlns="http://www.opengis.net/gml/3.2">https://register.geonorge.no/sosi-kodelister/fkb/ar5/5.0/arealressursarealtype</defaultCodeSpace>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">ARTYPE</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="treslag" type="gml:CodeType">
            <annotation>
              <documentation>type of wood: inndeling etter fordeling av kronedekket mellom bartrær og lauvtrær</documentation>
              <appinfo>
                <defaultCodeSpace xmlns="http://www.opengis.net/gml/3.2">https://register.geonorge.no/sosi-kodelister/fkb/ar5/5.0/arealressurstreslag</defaultCodeSpace>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">ARTRESLAG</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="skogbonitet" type="gml:CodeType">
            <annotation>
              <documentation>forestSiteQuality: inndeling etter arealets evne til å produsere trevirke</documentation>
              <appinfo>
                <defaultCodeSpace xmlns="http://www.opengis.net/gml/3.2">https://register.geonorge.no/sosi-kodelister/fkb/ar5/5.0/arealressursskogbonitet</defaultCodeSpace>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">ARSKOGBON</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="grunnforhold" type="gml:CodeType">
            <annotation>
              <documentation>groundConditions: inndeling etter tjukkelse, type og fordeling av jorddekket</documentation>
              <appinfo>
                <defaultCodeSpace xmlns="http://www.opengis.net/gml/3.2">https://register.geonorge.no/sosi-kodelister/fkb/ar5/5.0/arealressursgrunnforhold</defaultCodeSpace>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">ARGRUNNF</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="klassifiseringsmetode" type="gml:CodeType">
            <annotation>
              <documentation>metode for klassifisering av areal</documentation>
              <appinfo>
                <defaultCodeSpace xmlns="http://www.opengis.net/gml/3.2">https://register.geonorge.no/sosi-kodelister/fkb/ar5/5.0/klassifiseringsmetode</defaultCodeSpace>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">KLASSIFISERINGSMETODE</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element maxOccurs="unbounded" minOccurs="0" name="avgrensesAvArealressursGrense" type="app:ArealressursGrensePropertyType">
            <annotation>
              <documentation>Krav til delt flategeometri. Avgrensning av Arealressursflate ved hjelp av Arealressursgrense</documentation>
            </annotation>
          </element>
          <element maxOccurs="unbounded" minOccurs="0" name="agrensesAvArealressursGrenseFiktiv" type="app:ArealressursGrenseFiktivPropertyType">
            <annotation>
              <documentation>Krav til delt flategeometri. Avgrensning av Arealressursflate ved hjelp av bygningsdelelinje</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
